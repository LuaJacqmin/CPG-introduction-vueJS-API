RewriteEngine On
RewriteRule auth$ auth.php [L]
RewriteRule ^([a-zA-Z0-9]+)/([0-9]+)/fav=([0-9]+)$ index.php?page=$1&user=$2&fav=$3 [L]
RewriteRule ^([a-zA-Z0-9]+)/([0-9]+)/cat=([a-zA-Z0-9]+)$ index.php?page=$1&user=$2&cat=$3 [L]
RewriteRule ^([a-zA-Z0-9]+)/([0-9]+)/id=([0-9]+)$ index.php?page=$1&user=$2&id=$3 [L]
RewriteRule ^([a-zA-Z0-9]+)/([0-9]+)$ index.php?page=$1&user=$2 [L]